<?xml version="1.0" encoding="utf-8"?>
<ontology name="Countries2020Ontology">
  
  <sort name="sort_country" dynamic="true"/>
  <sort name="sort_region" dynamic="false"/>
  <sort name="sort_language" dynamic="true"/>
  <sort name="sort_regional_bloc" dynamic="false"/>
  
  <sort name="population_string" dynamic="true"/>
  <sort name="capital_string" dynamic="true"/>
  <sort name="neighbours_string" dynamic="true"/>
  <sort name="area_string" dynamic="true"/>
  <sort name="region_string" dynamic="true"/>
  <sort name="region_two_string" dynamic="true"/>
  <sort name="search_by_language_string" dynamic="true"/>
  <sort name="search_by_regional_bloc_string" dynamic="true"/>

  <!-- predicates: selected_X -- what the user can choose -->
  <predicate name="selected_country" sort="sort_country"/>
  <predicate name="selected_region" sort="sort_region"/>
  <predicate name="selected_language" sort="sort_language"/>
  <predicate name="selected_regional_bloc" sort="sort_regional_bloc"/>

  <!-- predicates: subjects: wh  -->
  <predicate name="population" sort="population_string"/>
  <predicate name="capital" sort="capital_string"/>
  <predicate name="neighbours" sort="neighbours_string"/>
  <predicate name="area" sort="area_string"/>
  <predicate name="language" sort="string"/>
  <predicate name="region" sort="region_string"/>
  <predicate name="region_two" sort="region_two_string"/>

  <!-- predicates: subjects: yn -->
  <!-- is selected_country in selected_region -->
  <predicate name="yn_region" sort="string"/>

  <!-- predicates: subjects: search_by-->
  <predicate name="search_by_language" sort="search_by_language_string"/>
  <predicate name="search_by_regional_bloc" sort="search_by_regional_bloc_string"/>


  <!-- individuals: regions -->
  
  <individual name="africa" sort="sort_region"/>
  <individual name="americas" sort="sort_region"/>
  <individual name="asia" sort="sort_region"/>
  <individual name="europe" sort="sort_region"/>
  <individual name="oceania" sort="sort_region"/>

  <!-- individuals: regional blocs -->
  <individual name="AL" sort="sort_regional_bloc"/>
  <individual name="ASEAN" sort="sort_regional_bloc"/>
  <individual name="AU" sort="sort_regional_bloc"/>
  <individual name="CAIS" sort="sort_regional_bloc"/>
  <individual name="CARICOM" sort="sort_regional_bloc"/>
  <individual name="CEFTA" sort="sort_regional_bloc"/>
  <individual name="EEU" sort="sort_regional_bloc"/>
  <individual name="EFTA" sort="sort_regional_bloc"/>
  <individual name="EU" sort="sort_regional_bloc"/>
  <individual name="NAFTA" sort="sort_regional_bloc"/>
  <individual name="PA" sort="sort_regional_bloc"/>
  <individual name="SAARC" sort="sort_regional_bloc"/>
  <individual name="USAN" sort="sort_regional_bloc"/>

  <!-- individuals: languages-->
  <individual name="es" sort="sort_language"/>
  <individual name="en" sort="sort_language"/>
  <individual name="fr" sort="sort_language"/>
  <individual name="el" sort="sort_language"/>
  <individual name="fi" sort="sort_language"/>

  <!-- individuals: countries-->
  <individual name="IRQ" sort="sort_country"/>
  <individual name="COG" sort="sort_country"/>
  <individual name="BGD" sort="sort_country"/>
  <individual name="LBY" sort="sort_country"/>
  <individual name="ERI" sort="sort_country"/>
  <individual name="ZMB" sort="sort_country"/>
  <individual name="BRA" sort="sort_country"/>
  <individual name="AUT" sort="sort_country"/>
  <individual name="REU" sort="sort_country"/>
  <individual name="GLP" sort="sort_country"/>

